{
  "name": "insanprihatin",
  "version": "1.0.0",
  "description": "Yayasan Insan Prihatin - A prestigious foundation focused on community service",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch",
    "db:push": "dotenv -e .env.local -- drizzle-kit push",
