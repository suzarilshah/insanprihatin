    "@neondatabase/serverless": "^1.0.2",
    "@react-email/components": "^1.0.6",
    "@tailwindcss/postcss": "^4.1.18",
    "appwrite": "^21.5.0",
    "better-auth": "^1.4.18",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.45.1",
    "framer-motion": "^12.29.2",
    "jose": "^6.1.3",
    "next": "^16.1.6",
    "pg": "^8.17.2",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "resend": "^6.9.1"
